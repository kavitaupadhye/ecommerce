	<listeners>
		<listener class-name="com.selenium.report.ListenerCode" />
		<listener class-name="com.selenium.report.ExtentTestNGIReporterListener" />
	</listeners>



	<test name="TC 01 - fixed deposit">
		<classes>
			<class name="com.selenium.keyworddriven.FixedDeposit_Test" />
		</classes>
	</test> <!-- RegressionTest -->


	<test name="TC 02 - calculator">
		<classes>
			<class name="com.selenium.keyworddriven.Calculator_Test" />
		</classes>
	</test> <!-- RegressionTest -->



	<test name="TC 03 - blaze-demo ">
		<classes>
			<class name="com.selenium.keyworddriven.BlazeDemo_Test" />
		</classes>
	</test> <!-- RegressionTest -->


	



</suite> <!-- Suite -->
